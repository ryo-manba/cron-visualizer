{
  "name": "cron-visualizer",
  "version": "0.1.0",
  "private": true,
  "description": "A react app to visualize cron expressions",
  "keywords": [
    "react",
    "cron",
    "cron expression",
    "cron visualizer"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "start": "react-app-rewired start",
    "build": "pnpm build:wasm && react-app-rewired build",
    "build:wasm": "cd cron-wasm && wasm-pack build --target bundler",
    "test": "react-app-rewired test",
    "test:wasm": "cd cron-wasm && cargo test",
    "eject": "react-scripts eject",
    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "format": "prettier --write '*/**/*.{js,ts,tsx}'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "13.5.0",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "@vercel/analytics": "^1.5.0",
    "autoprefixer": "10.4.14",
    "cron-parser": "4.8.1",
    "cron-wasm": "link:cron-wasm/pkg",
    "moment-timezone": "0.5.43",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.9.0",
    "react-scripts": "5.0.1",
    "react-tooltip": "5.13.1",
    "recharts": "2.6.2",
    "tailwindcss": "3.3.2",
    "typescript": "4.9.5",
    "web-vitals": "2.1.4"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.21.8",
    "@jridgewell/sourcemap-codec": "1.4.15",
    "@rollup/plugin-terser": "0.4.3",
    "@types/jest": "27.5.2",
    "@types/node": "22.18.8",
    "@types/react-tooltip": "^4.2.4",
    "@types/testing-library__jest-dom": "5.14.6",
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "@typescript-eslint/parser": "5.59.8",
    "eslint": "8.41.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "prettier": "2.8.8",
    "react-app-rewired": "^2.2.1",
    "svgo": "3.0.2",
    "workbox-cacheable-response": "7.0.0"
  },
  "engines": {
    "node": "22.20.0",
    "pnpm": ">=8.6.0"
  },
  "packageManager": "pnpm@8.6.0",
  "volta": {
    "node": "22.20.0"
  }
}
